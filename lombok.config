# Lombok Configuration for Flink Platform
# This configuration enables null safety with JSpecify annotations

# Disable automatic null annotation generation
# We'll use JSpecify annotations explicitly instead
lombok.addNullAnnotations = none

# Add @lombok.Generated annotation to generated methods
# This helps IDEs and tools recognize generated code
lombok.addLombokGeneratedAnnotation = true

# Add @SuppressWarnings("all") to generated code
# This suppresses warnings for generated code
lombok.addSuppressWarnings = true

# Copy non-nullness annotations from parameters to generated code
lombok.copyableAnnotations += org.jspecify.annotations.Nullable
lombok.copyableAnnotations += org.jspecify.annotations.NonNull
